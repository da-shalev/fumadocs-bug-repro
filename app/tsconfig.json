{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["dom", "dom.iterable", "esnext"],
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "jsx": "react-jsx",
    "isolatedModules": true,
    "baseUrl": ".",
    "paths": {
      "fumadocs-mdx:collections/*": [".source/*"]
    }
  },
  "include": ["repro.ts", "source.ts", ".source/**/*.ts"],
  "exclude": ["node_modules"]
}
